      </Fields>
      <Query>
        <DataSourceName>LCLAdmin</DataSourceName>
        <CommandText>SELECT  SUM(totalplanannee) AS totalplanannee ,
        SUM(totalplanamois) AS totalplanamois,
        SUM(ytd) AS ytd,
        SUM(month_amount) AS month_amount
FROM
        (SELECT catid,
                rptcat,
                rptdesc,
                SUM(totalplanannee) /12 *
                (SELECT MOISFY
                FROM    YEARCONVERT
                WHERE   MOIS = @Month
                ) AS totalplanannee ,
                SUM(totalplanamois) AS totalplanamois,
                SUM(month_amount) AS month_amount ,
                SUM(ytd) AS ytd
       FROM dbo.NomisDept(@Month, @Year) AS NOMS
        FULL JOIN dbo.budget(@Month, @Year) BUDG
        ON      BUDG.year         = noms.year
            AND budg.gl           = noms.gl
            AND budg.departmentid = noms.departmentid
            WHERE MONTH             =
                (SELECT MOISFY
                FROM    YEARCONVERT
                WHERE   MOIS = @Month
                )
            AND NOMS.YEAR = @Year +
                (SELECT MULTI
                FROM    YEARCONVERT
                WHERE   MOIS = @Month
                )
        GROUP BY catid,
                rptcat,
                rptdesc
        ) a
WHERE   catid = 2
GROUP BY catid</CommandText>
