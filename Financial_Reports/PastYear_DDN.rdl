  <Code>public function ShowPourcentage(byval budget as String, byval diff as String) as String
	dim reponse as String
	if budget = " " or diff = " "then
		reponse =" "
	else
		reponse = format(ctype((cdbl(diff)/cdbl(budget)),String),"Percent")
	end if

	return reponse 
end function

Public Shared Function DivideXbyY(ByVal X As Object, ByVal Y As Object) As Decimal
on error goto divide_Error
    DivideXbyY = IIF(Y=0, 0, X / IIF(Y=0, 1, Y))
exit function
divide_Error:
    DivideXbyY = 0	
End Function</Code>
