      <Fields>
        <Field Name="department_name">
          <DataField>department_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="totalplanannee">
          <DataField>totalplanannee</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="totalplanamois">
          <DataField>totalplanamois</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="month_amount">
          <DataField>month_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ytd">
          <DataField>ytd</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LCLAdmin</DataSourceName>
        <CommandText>SELECT  department_name,
        SUM(case when catid = 1 then totalplanannee * -1
	         else totalplanannee end) /12 * (SELECT MOISFY FROM YEARCONVERT WHERE   MOIS = @Month) AS totalplanannee ,
        SUM(case when catid = 1 then totalplanamois * -1
	         else totalplanamois end) AS totalplanamois,
        SUM(month_amount) AS month_amount ,
        SUM(ytd) AS ytd
FROM    dbo.NomisDept(@Month, @Year) AS NOMS
        FULL JOIN dbo.budget(@Month, @Year) BUDG
        ON      BUDG.year         = noms.year
            AND budg.gl           = noms.gl
            AND budg.departmentid = noms.departmentid
WHERE   MONTH                     =
        (SELECT MOISFY
        FROM    YEARCONVERT
        WHERE   MOIS = @Month
        )
    AND NOMS.YEAR = @Year +
        (SELECT MULTI
        FROM    YEARCONVERT
        WHERE   MOIS = @Month
        )
and catid &lt;= 6
GROUP BY department_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
