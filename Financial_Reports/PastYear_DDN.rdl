        </Field>
        <Field Name="month_amount">
          <DataField>month_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ytd">
          <DataField>ytd</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="past_month_amount">
          <DataField>past_month_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="past_ytd">
          <DataField>past_ytd</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LCLAdmin</DataSourceName>
        <CommandText>SELECT GL.catid,
       GL.rptcat, 
       GL.rptdesc,
       sum(NOMS.month_amount) as month_amount,
       sum(NOMS.ytd) as ytd,
       sum(PAST.month_amount) as past_month_amount,
       sum(PAST.ytd) as past_ytd
from dbo.NomisDept(@Month, @Year) AS NOMS
        INNER JOIN dbo.NomisDept(@Month, @pyear) PAST
	ON      PAST.year + 1     = noms.year 
            AND PAST.gl           = noms.gl
            AND PAST.departmentid = noms.departmentid
		INNER JOIN TBLGL GL
		ON GL.GL = NOMS.GL
  WHERE NOMS.MONTH = (SELECT MOISFY FROM YEARCONVERT WHERE MOIS = @Month)
  AND NOMS.YEAR  = @Year + (SELECT MULTI FROM YEARCONVERT WHERE MOIS = @Month)
  and catid &lt;=6
group by  GL.catid,GL.rptcat, GL.rptdesc
order by catid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Month">
            <Value>=Parameters!Month.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pyear">
            <Value>=Parameters!pyear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
